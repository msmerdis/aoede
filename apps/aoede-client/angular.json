{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": ".",
  "projects": {
    "aoede-client-generic": {
      "projectType": "library",
      "root": "aoede-client-generic",
      "sourceRoot": "aoede-client-generic/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "aoede-client-generic/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "aoede-client-generic/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "aoede-client-generic/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "aoede-client-generic/src/test.ts",
            "tsConfig": "aoede-client-generic/tsconfig.spec.json",
            "karmaConfig": "aoede-client-generic/karma.conf.js"
          }
        },
        "deploy": {
          "builder": "ngx-deploy-npm:deploy",
          "options": {
            "access": "public",
            "buildTarget": "production"
          }
        }
      }
    },
    "aoede-client": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/aoede-client",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "aoede-client:build:production"
            },
            "development": {
              "browserTarget": "aoede-client:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "aoede-client:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "aoede-client-music": {
      "projectType": "library",
      "root": "aoede-client-music",
      "sourceRoot": "aoede-client-music/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "aoede-client-music/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "aoede-client-music/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "aoede-client-music/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "aoede-client-music/src/test.ts",
            "tsConfig": "aoede-client-music/tsconfig.spec.json",
            "karmaConfig": "aoede-client-music/karma.conf.js"
          }
        },
        "deploy": {
          "builder": "ngx-deploy-npm:deploy",
          "options": {
            "access": "public",
            "buildTarget": "production"
          }
        }
      }
    },
    "aoede-client-user": {
      "projectType": "library",
      "root": "aoede-client-user",
      "sourceRoot": "aoede-client-user/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "aoede-client-user/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "aoede-client-user/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "aoede-client-user/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "aoede-client-user/src/test.ts",
            "tsConfig": "aoede-client-user/tsconfig.spec.json",
            "karmaConfig": "aoede-client-user/karma.conf.js"
          }
        },
        "deploy": {
          "builder": "ngx-deploy-npm:deploy",
          "options": {
            "access": "public",
            "buildTarget": "production"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
